{
  "name": "@jotter/dateformat",
  "version": "1.0.2",
  "description": "Lightweight and easy-to-use date/time formatting function that returns the formatted date/time based on the passed in placeholders.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf -rf ./dist",
    "build": "npm run clean && NODE_ENV=production rollup -c rollup.config.mjs",
    "test": "jest"
  },
  "keywords": [
    "dateFormat",
    "date formatting",
    "date",
    "time",
    "datetime",
    "formatting",
    "from now",
    "to now",
    "time ago"
  ],
  "homepage": "https://github.com/Meqn/jotter/tree/main/libs/dateFormat",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meqn/jotter.git"
  },
  "author": "lmq <mengqing723@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Meqn/jotter/issues"
  }
}
