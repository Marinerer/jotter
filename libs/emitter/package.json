{
	"name": "@jotter/emitter",
	"version": "1.1.2",
	"description": "Simple and modern emitter library.",
	"type": "module",
	"main": "dist/index.js",
	"unpkg": "dist/index.min.js",
	"jsdelivr": "dist/index.min.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"module": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./*": "./dist/*"
	},
	"files": [
		"dist"
	],
	"buildOptions": {
		"name": "EventEmitter",
		"input": "index.ts",
		"target": "ES2015"
	},
	"keywords": [
		"event",
		"emitter",
		"pubsub",
		"listener",
		"emit",
		"trigger",
		"dispatchEvent",
		"event emitter",
		"event bus"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"sideEffects": false,
	"homepage": "https://github.com/Marinerer/jotter/tree/main/libs/emitter",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Marinerer/jotter.git",
		"directory": "libs/emitter"
	},
	"author": "Mariner <mengqing723@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Marinerer/jotter/issues"
	}
}
