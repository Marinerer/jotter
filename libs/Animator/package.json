{
	"name": "@jotter/animator",
	"version": "1.0.3",
	"description": "Animator is an animation playback controller. It makes it easy to create and control JavaScript animations.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"unpkg": "dist/index.min.js",
	"jsdelivr": "dist/index.min.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.esm.js",
			"types": "./dist/index.d.ts"
		},
		"./*": "./*"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rimraf -rf ./dist",
		"build": "pnpm run clean && NODE_ENV=production rollup -c rollup.config.mjs",
		"test": "jest"
	},
	"keywords": [
		"animator",
		"animate",
		"animation",
		"animation frame",
		"animation player",
		"animation controller",
		"requestAnimationFrame"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"sideEffects": false,
	"homepage": "https://github.com/Meqn/jotter/tree/main/libs/animator",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Meqn/jotter.git"
	},
	"author": "lmq <mengqing723@gmail.com>",
	"license": "MIT"
}
